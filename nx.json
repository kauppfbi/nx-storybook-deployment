{
  "npmScope": "kauppfbi",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "ui-header": {
      "tags": []
    },
    "ui-header-e2e": {
      "tags": [],
      "implicitDependencies": ["ui-header"]
    },
    "test-app": {
      "tags": []
    },
    "test-app-e2e": {
      "tags": [],
      "implicitDependencies": ["test-app"]
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
